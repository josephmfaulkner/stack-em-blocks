{
  "name": "stack-em-blocks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^4.0.0",
    "hammerjs": "^2.0.8",
    "mousetrap": "^1.6.5",
    "move-file-cli": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "web-vitals": "^0.2.4",
    "yarn-or-npm": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-test": "dotenv -e .env.test react-scripts build && move-file build bin/test",
    "build-prod": "dotenv -e .env.prod react-scripts build && move-file build bin/prod",
    "build-all": "yarn-or-npm run build-test && yarn-or-npm run build-prod",
    "lint": "node_modules/.bin/eslint --ext js src",
    "test": "react-scripts test",
    "run-unit-tests": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools": "^3.7.0",
    "redux-saga-test-plan": "^4.0.1"
  }
}
